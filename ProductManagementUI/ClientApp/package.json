{
  "name": "product-definition",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "ng lint --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.6",
    "@angular/cdk": "^14.1.0",
    "@angular/common": "^14.2.6",
    "@angular/compiler": "^14.2.6",
    "@angular/core": "^14.2.6",
    "@angular/forms": "^14.2.6",
    "@angular/material": "^14.1.0",
    "@angular/material-moment-adapter": "^14.1.0",
    "@angular/platform-browser": "^14.2.6",
    "@angular/platform-browser-dynamic": "^14.2.6",
    "@angular/router": "^14.2.6",
    "@azure/msal-angular": "^2.4.5",
    "@azure/msal-browser": "^2.30.0",
    "angular-font-awesome": "^3.1.2",
    "convert-csv-to-json": "^1.3.3",
    "jsondiffpatch": "^0.4.1",
    "mat-table-exporter": "^10.2.4",
    "moment": "^2.29.4",
    "ng-multiselect-dropdown": "^1.0.0-beta.19",
    "ngx-csv": "^0.3.2",
    "ramda": "^0.28.0",
    "rxjs": "~7.5.0",
    "text-diff": "^1.0.1",
    "tslib": "^2.4.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.5",
    "@angular-eslint/builder": "14.0.2",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
    "@angular-eslint/schematics": "14.0.2",
    "@angular-eslint/template-parser": "14.0.2",
    "@angular/cli": "~14.0.5",
    "@angular/compiler-cli": "^14.2.6",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "eslint": "^8.18.0",
    "jest": "^28.1.3",
    "jest-preset-angular": "^12.2.0",
    "path-browserify": "^1.0.1",
    "simple-text-diff": "^1.7.0",
    "typescript": "~4.7.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup.jest.ts"
    ],
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsConfig.spec.json",
        "stringifyContentPathRegex": "\\.html$"
      }
    }
  }
}
